<html>
<body>
<table>
    <tr>
        <td>Id</td>
        <td>Title</td>
        <td>dueDate</td>
        <td>Completed</td>
    </tr>
    <% for(var i=0; i< allTodos.length; i++){%>
    <tr>
        <td>
            <%=allTodos[i].id %>
        </td>
        <td>
            <%=allTodos[i].title %>
        </td>
        <td>
            <%=allTodos[i].dueDate %>
        </td>
        <td>
            <%=allTodos[i].completed %>
        </td>
    </tr>
<% } %>
</table>
</body>
</html>